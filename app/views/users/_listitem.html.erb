<tr>
  <td> <%= order.item.name %></td>
  <td><%=  number_to_currency(order.item.price) %></td>
  <td><%= order.quantity %></td>
  <td> <%= number_to_currency(order.item.price * order.quantity)%></td>
  <td> <%= link_to "Edit", edit_order_path(order) %></td>
  <td><%= link_to "Delete", order_path(order), method: :delete, data: { confirm: "Are you sure you want to delete this order?" } %></td>
</tr>
